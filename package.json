{
  "name": "kimono-app-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next -p 3100",
    "dev:css": "postcss src/styles/index.css -o src/styles/bundle.css -w",
    "build": "yarn run build:css && next build",
    "build:css": "postcss src/styles/index.css -o src/styles/bundle.css",
    "start": "next start",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'; stylelint 'src/**/*.{css,jsx,tsx}';",
    "format": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'; stylelint --fix 'src/**/*.{css,jsx,tsx}';",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:ci": "jest --runInBand --collect-coverage",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o build/storybook"
  },
  "dependencies": {
    "@aws-amplify/ui-components": "^0.8.2",
