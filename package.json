{
  "name": "kimono-app-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next -p 3100",
    "dev:css": "postcss src/styles/index.css -o src/styles/bundle.css -w",
    "build": "yarn run build:css && next build",
    "build:css": "postcss src/styles/index.css -o src/styles/bundle.css",
    "start": "next start",
    "lint:prettier": "prettier 'src/**/*.{js,jsx,ts,tsx,css,json}' --check",
    "fix:prettier": "yarn lint:prettier --write",
    "lint:eslint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "fix:eslint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:stylelint": "stylelint 'src/**/*.{css,jsx,tsx}'",
    "fix:stylelint": "stylelint --fix 'src/**/*.{css,jsx,tsx}'",
    "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:stylelint",
    "format": "yarn fix:prettier && yarn fix:eslint && yarn fix:stylelint",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:ci": "jest --runInBand --collect-coverage",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o build/storybook",
    "deploy:stg": "cp stg.serverless.yml serverless.yml && AWS_PROFILE=kimono-app-stg serverless -v",
    "remove:stg": "cp stg.serverless.yml serverless.yml && AWS_PROFILE=kimono-app-stg serverless remove -v"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "aws-amplify": "^3.3.4",
    "extensible-custom-error": "^0.0.7",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "next": "^9.5.5",
    "nookies": "^2.4.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^4.0.0",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@storybook/addon-a11y": "^6.0.27",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-docs": "^6.0.27",
    "@storybook/addon-knobs": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/addon-storyshots": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/node": "^14.14.2",
    "@types/prettier": "^2.1.5",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.3",
    "@types/redux-logger": "^3.0.8",
    "@types/stylelint": "^9.10.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@zeit/next-css": "^1.0.1",
    "babel-preset-react-app": "^10.0.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.12.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.1",
    "jest-fetch-mock": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.1.2",
    "prettier-stylelint": "^0.4.2",
    "raw-loader": "^4.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-test-renderer": "^16.14.0",
    "serverless": "^2.17.0",
    "stylelint": "^13.7.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3",
    "yargs": "^16.1.0"
  },
  "license": "MIT"
}
